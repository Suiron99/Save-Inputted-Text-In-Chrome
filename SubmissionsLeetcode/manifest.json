{
  "name": "Saving Text",
  "description": "Saving Text",
  "version": "1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    }
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "options.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        ""
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}